{
  "metadata": {
    "release": {
      "name": "mymongo",
      "namespace": "demo"
    }
  },
  "spec": {
    "version": "4.3.2",
    "mode": "Standalone",
    "replicas": 3,
    "replicaSet": {
      "name": "rs0"
    },
    "shardTopology": {
      "shard": {
        "replicas": 3,
        "shards": 3,
        "persistence": {
          "size": "10Gi"
        }
      },
      "configServer": {
        "replicas": 3,
        "persistence": {
          "size": "2Gi"
        }
      },
      "mongos": {
        "replicas": 3
      }
    },
    "clusterAuthMode": "keyFile",
    "sslMode": "disabled",
    "terminationPolicy": "WipeOut",
    "storageClass": {
      "name": "standard"
    },
    "persistence": {
      "size": "10Gi"
    },
    "machine": "db.t.micro",
    "resources": null,
    "authSecret": {
      "create": true,
      "annotations": {},
      "name": "",
      "password": ""
    },
    "serviceAccount": {
      "create": true,
      "annotations": {},
      "name": ""
    }
  }
}